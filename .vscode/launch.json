{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Extract Only",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/camreader.py",
        "args": ["input/Save0.cam", "output"],
        "console": "integratedTerminal"
